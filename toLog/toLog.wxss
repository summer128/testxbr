.head{
  width: 100%;
  height: 58rpx;
  background-color: #fff;
}
.head text{
  display: block;
  width: 94%;
  height: 100%;
  line-height: 58rpx;
  text-align: right;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.imgbox{
  width: 100%;
  margin-top: 200rpx;
}
.imgbox image{
  width: 435rpx;
  height: 262rpx;
  margin: 0 auto;
  display: block;
}
.hintbox{
  width: 100%;
  height: 26rpx;
  margin-top: 30rpx;
}
.hintbox view{
  width: 360rpx;
  height:26rpx;
  font-size:28rpx;
  margin: 0 auto;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height: 26rpx;
}
.loginbox{
  width: 100%;
  height: 80rpx;
}
.loginbox view{
  width: 300rpx;
  height: 100%;
  margin: 50rpx auto 0;
  background:linear-gradient(90deg,rgba(135,213,36,1),rgba(111,222,38,1));
  box-shadow:0rpx 9rpx 20rpx 0rpx rgba(153,204,51,0.25);
  border-radius:40rpx;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
}