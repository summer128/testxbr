.imgbox{
  width: 100%;
  height: 200rpx;
  margin-top: 300rpx;
}
.imgbox image{
  width: 300rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
}
.suc_title{
  width: 100%;
  height: 42rpx;
  margin-top: 50rpx;
}
.suc_title view{
  width: 185rpx;
  height: 42rpx;
  margin: 0 auto;
  line-height: 42rpx;
  font-size:44rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.suc_con{
  width: 100%;
  height: 29rpx;
  margin-top: 30rpx;
}
.suc_con view{
  width: 300rpx;
  height: 29rpx;
  margin: 0 auto;
  line-height: 29rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.back{
  width: 100%;
  height:80rpx;
  margin-top: 100rpx;
}
.footer{
  width: 88%;
  height: 100%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.footer_l{
  width: 300rpx;
  height: 100%;
  text-align: center;
  line-height: 80rpx;
  background:linear-gradient(90deg,rgba(135,213,36,1),rgba(111,222,38,1));
  border-radius:40rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.footer_r{
  width: 300rpx;
  height: 100%;
  text-align: center;
  line-height: 80rpx;
  border:1rpx solid rgba(135,213,52,1);
  border-radius:40rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(135,213,52,1);
}