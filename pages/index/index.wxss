.background{
  background: white;
}

.health_container{
  width:100%;
  background: #ffffff;
  border-radius: 30rpx;
  padding: 33rpx 0;

}
.health_head,.leftright_center{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  overflow:hidden
}
.center_title{
  text-align: left;
  font-weight: bold;
  font-size: 32rpx;
}
.right_refresh{
  width: 90rpx;
  height: 44rpx;
  float: right;
  border-radius: 22rpx;
  text-align: center;
  line-height: 44rpx;
  font-size: 24rpx;
  border: 3rpx solid rgba(240, 240, 240, 1);
  margin-top: 20rpx;
  margin-right: 24rpx;
}

.health_ico,.health_refresh{
  width: 60rpx;
  height: 100rpx;
}
.health_refresh_again{
  transition:1s;
  transform: rotateZ(360deg);
}
.health_ico{
  float: left;
  padding-left: 24rpx;
  margin-top: 32rpx;
}
.health_refresh{
  float: right;
}
.green,.yellow{
  width:20rpx;
  height:20rpx;
  background:rgba(1,203,154,1);
  border-radius:50%;
  
}
.green{
  float: left;
  transform: translate(8rpx);
}
.yellow{
  float: left;
  background:rgba(248,181,81,1);
}

.health_refresh image{
  width: 30rpx;
  height: 36rpx;
}
.health_title{
  text-align: center;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:#333333;
}
.health_wt{
  color:#333333;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:500;
  padding: 12rpx 32rpx;
}
.health_answers{
  padding: 12rpx 0;
  padding-left: 24rpx;
  padding-right: 39rpx;
  color:#333333;
  font-size: 26rpx;
  font-family:PingFang SC;
}



.x_list{
  width: 100%;
  border-radius: 0 0 30rpx 30rpx;
  height: 220rpx;
  /* margin: 0 auto; */
  background-color: #ffffff;
  margin-bottom: 30rpx;
  display: flex;
}
.head_tab{
  width: 112rpx;
