  line-height: 44rpx;
  font-size: 24rpx;
  border: 3rpx solid rgba(240, 240, 240, 1);
  position: absolute;
  right: 0;
  top: 43rpx;
  
}
.list_one{
  width: 94%;
  margin: 0 auto;
  height: 246rpx;
  position: relative;
  border-bottom: 1rpx solid rgba(238, 238, 238, 1);
}
.one_img{
  width: 160rpx;
