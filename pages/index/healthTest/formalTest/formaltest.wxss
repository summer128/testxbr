/* 首页的两圈图标 */
.health_icos{
  width: 6%;
  height: 40rpx;
  float: left;
  /* padding-left: 24rpx; */
  margin-top: 12rpx;
}
.green,.yellow{
  width:20rpx;
  height:20rpx;
  background:rgba(1,203,154,1);
  border-radius:50%;
  
}
.green{
  float: left;
  transform: translate(8rpx);
}
.yellow{
  float: left;
  background:rgba(248,181,81,1);
}


.test_conntainer{
  padding: 20rpx;
}
.test_problem{
  width: 100%;
  /* height: 40rpx; */
  padding: 10rpx 0;
  float: left;
  margin-top: 24rpx;
}
.problem_head{
  overflow: hidden;
}
.problems{
  width: 92%;
  color: #333333;
  float: left;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 40rpx;
  padding-left: 10rpx;
}
.select_you{
  font-size: 28rpx;
  color: #A0A0A0;
  padding-left: 78rpx;
  line-height: 36rpx;
  /* padding:11rpx */
}
.select_answer{
  /* margin:68rpx  68rpx  32rpx 68rpx; */
  width: 614rpx;
  height: 120rpx;
  border-radius:10rpx;
  line-height: 30rpx;
  
  margin:68rpx 48rpx 32rpx 48rpx;
  box-shadow:0rpx 0rpx 30rpx 0rpx #5D5D5D;

}
.yesorno{
  width: 460rpx;
  float: left;
  /* line-height: 120rpx; */
  padding-top: 46rpx;
  color: #333333;
  font-size: 28rpx;
  padding-left: 30rpx;
}
.test_sign{
  width: 124rpx;
  height: 120rpx;
  float: right;
  text-align: center;
  line-height: 120rpx;
  color: #F6810E;
  font-size: 90rpx;
  border-left:1px solid rgba(238,238,238,1);
}
.opacity_sign{
  opacity: 0.4;
  width: 124rpx;
  height: 120rpx;
  float: right;
  text-align: center;
  line-height: 120rpx;
  color: #F6810E;
  font-size: 90rpx;
  border-left:1px solid rgba(238,238,238,1);
}

.test_nums{
  width: 82%;
  height: 50rpx;
  line-height: 50rpx;
  color: #999999;
  font-size: 32rpx;
  text-align: right;
  /* float: right; */
  padding:0 68rpx;
  margin-bottom: 40rpx;
}
.test_submit{
  width: 100%;
  border-radius: 40rpx;
}