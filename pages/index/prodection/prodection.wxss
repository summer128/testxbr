.headbox{
  width: 100%;
  height: 100rpx;
  background-color: #87D534;
}
.background{
  position: relative;
  top: -5rpx;
}
#shar{
  margin: 0;
  width: 50rpx;
  background-color: #fff;
  padding: 0;
}
.head{
  width: 94%;
  height: 100%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}
.a{
  width: 80rpx;
  height: 60rpx;
  margin-top: 20rpx;
  line-height: 60rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.active{
  border-bottom: 6rpx solid #fff;
  font-size: 34rpx;
  font-weight: bold;
}
.list_one{
  width: 93.6%;
  /* height: 600rpx; */
  background-color: #fff;
  margin: 20rpx auto 0;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  padding: 20rpx 0;
}
.con{
  width: 94%;
  margin: 0 auto;
}
.con1{
  height:36rpx;
  background:rgba(245,245,245,1);
  border-radius:5rpx;
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  text-align: left;
  line-height: 36rpx;
  margin-top: 30rpx;
}
.con2{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight: bold;
  color:#333333;
  margin-top: 26rpx;
}
.mt1,.mt2{
  margin-top: 35rpx;
}
.con3{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-top: 16rpx;
}
.con4{
  margin-top: 10rpx;
  height: 300rpx;
}
.con5{
  width: 93.6%;
  height:1rpx;
  background:rgba(238,238,238,1);
  /* border:1rpx solid rgba(238,238,238,1); */
  margin-top: 50rpx;
}
.con6{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:#999999;
  /* margin-top: 40rpx; */
  display: flex;
  justify-content: space-between;
}
.con6 image{
  width: 28rpx;
  height: 28rpx;
}
