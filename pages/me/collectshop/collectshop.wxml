<view class='box'>
  <block wx:for="{{delList}}" wx:key="index">
  <view class='box_item' data-id="{{item.id}}">
    <view class='movebox'>
      <movable-area>
        <movable-view direction="horizontal" x="{{curr==item.id?x:''}}" data-id="{{item.goodsId}}" bindtouchstart='touchStart' bindtouchend='touchEnd' out-of-bounds="{{true}}" friction="150">
          <view class="contentbox">
                <view class="warp">
                    <view class="imgbox">
                        <image src="{{item.url}}"></image>
                    </view>
                    <view class="contentfont">
                        <view class="contentname">{{item.name}}</view>
                        <view class="contentprice">{{item.price}}</view>
                    </view>
                </view>
          </view>
         </movable-view>
      </movable-area>
      <view class='delbox' data-delID="{{item.goodsId}}" catchtap='del'>
        删除
      </view>
    </view>
  </view>
</block>
</view>


