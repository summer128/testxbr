
<view class="logistics-container">
  <view class="logistics-info">
    <view class="logistics-box">
      <view class="logistics-item" wx:for="{{wuliu}}" wx:key="index">
        <view class="times">123</view>
        <view class="logistics-item-left">
          <view class="line-box">
            <view class="top-line"></view>
            <view class="bottom-line"></view>
          </view>
          <view class="center-circle"></view>
        </view>
        <view class="logistics-item-right ">
          <view class="logistics-item-text">{{item.context}}</view>
          <view class="logistics-item-time">{{item.time}}</view>
        </view>
      </view>
    </view>
  </view>
</view>

